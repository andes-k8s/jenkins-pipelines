@Library('andes-library') _
pipeline {
  environment {
    registryCredential = "dockerhub"
  }
  agent {
    kubernetes {
      inheritFrom 'kube-agent'
      yamlFile '../agent-kubernetes/dockerPod.yaml'
    }
  }
  stages {
    stage('Build API') {
      steps {
        script {
          node{
            andesRunTests{
              repoUrl = "https://github.com/andes/andes-test-integracion.git"
            }
          }
        }
      }
    }
  }
}

